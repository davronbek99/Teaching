<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="true">
        <shape>
            <corners android:radius="8dp" />
            <solid android:color="@color/white" />
            <stroke android:width="1dp" android:color="@color/material_dynamic_neutral90" />
        </shape>
    </item>

    <item android:state_enabled="false">
        <shape>
            <corners android:radius="8dp" />
            <stroke android:width="1dp" android:color="@color/secondary_button_border_color" />
            <solid android:color="@color/brand_main_wild_sand" />
        </shape>
    </item>
</selector>